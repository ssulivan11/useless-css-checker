{
  "name": "useless-css-checker",
  "version": "0.2.0",
  "license": "MIT",
  "description": "Unused CSS Checker. Built off of PurifyCSS",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest tests/index.spec.js",
    "build": "webpack -p",
    "lint": "eslint src --fix",
    "prettier": "prettier --write '**/*.js'",
    "prepublish": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn build && yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "clean-css": "^4.0.12",
    "colors": "^1.2.1",
    "glob": "^7.1.1",
    "node-emoji": "^1.8.1",
    "rework": "^1.0.1",
    "uglify-js-webpack-build": "^3.3.22",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.3",
    "prettier": "^1.19.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "sha.js": "2.4.11",
    "upath": "1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ssulivan11/useless-css-checker.git"
  },
  "jest": {
    "testURL": "http://localhost"
  }
}