{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {},
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "arrow-parens": [
      "error",
      "always"
    ],
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "space-before-function-paren": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-restricted-syntax": 0,
    "id-length": [
      2,
      {
        "exceptions": [
          "e",
          "i"
        ]
      }
    ],
    "prefer-destructuring": 0,
    "import/no-extraneous-dependencies": [
      2,
      {
        "devDependencies": true
      }
    ],
    "import/no-named-as-default-member": 0,
    "no-continue": 0
  }
}